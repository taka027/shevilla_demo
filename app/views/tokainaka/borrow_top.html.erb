
<div class="contents">

   <nav id="breadcrumbs">
     <ol>
       <li itemscope="itemscope" itemtype="http://data-vocabulary.org/Breadcrumb">
         <a itemprop="url" href="/"><span itemprop="title">ホーム</span></a>
       </li>
       <li itemscope="itemscope" itemtype="http://data-vocabulary.org/Breadcrumb">
         <a itemprop="url" href="/"><span itemprop="title">借りたい</span></a>
       </li>
     </ol>
   </nav>

   <%= render 'search_form' %>

   <section class="villa_list">
      <div class="villa_list__inner clearfix"><!---------villa_list__inner_start------------>

         <% @villa.each do |obj| %>
         <div class="villa_list__card">
            <h2 class="villa_list__card-tit font-b"><%= obj.title %></h2>
            <figure><%= image_tag('borrow/villa_img.png', :alt => "物件イメージ") %></figure>
            <div class="owner_description">
               <div class="owner">
                  <div class="owner_inner">
                     <span class="owner_img"><%= image_tag('detail/owner_img.png', :alt => "オーナー画像") %></span>
                     <div class="owner_info">
                        <dl class="area_name font-b">
                           <dt class="area"><%= obj.tarea.name %></dt>
                           <dd class="user_name">ユーザーネーム</dd>
                        </dl>
                        <span class="star"><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i></span>
                     </div>
                  </div>
                  <div class="villa_description">
                     <span class="price font-b">\<%= obj.price %>~</span>
                     <span class="use_price">（1日の利用料金）</span>
                  </div>
               </div>
               <ul class="category_list">
                  <li><%= obj.tcategory.name %></li>
               </ul>
                <%= link_to "/detail/#{obj.id}", :class => 'detail_btn' do %>詳細を見る<i class="fa fa-chevron-right"></i><% end %>
            </div>
         </div>
         <% end %> 

         </div><!---------villa_list__inner_end------------>
   </section>






   <div class="illust_image"></div>
   <footer>
      <div class="footer_inner">
         <div class="footer_logo"><span class="font-cw">とかいなか×別荘シェアリングサービス</span><br><%= image_tag('common/footer_logo.jpg', :alt => "とかいなかのShavilla") %></div>
         <ul class="footer_nav">
            <li><a href="#">検索</a></li>
            <li><a href="#">借りたい</a></li>
            <li><a href="#">貸したい</a></li>
            <li><a href="#">運営</a></li>
            <li><a href="#">利用規約</a></li>
            <li><a href="#">お問い合わせ</a></li>
         </ul>
         <ul class="footer__sns">
            <li><a href="#" class="sns_twitter">twitter</a></li>
            <li><a href="#" class="sns_facebook">facebook</a></li>
         </ul>
<a href="#" id="pageTop">トップへ戻る</a>
      </div>
      <small class="copyright">Copyright (C) marutto-web-studio. All rights reserved.</small>
   </footer>
</div>
   
   
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<script src="js/delighters.js" type="text/javascript"></script>
<script src="js/stickyfill.min.js" type="text/javascript"></script>

<script type="text/javascript">
/*-------------globalmenu-----------*/
$(function(){
			$('#nav_toggle').click(function(){
						$("header").toggleClass('open');
				$("nav").fadeToggle(500);
			});
});
/*-------------pagetop-----------*/
$(function(){
  var pageTop = $("#pageTop");
  pageTop.click(function () {
    $('body, html').animate({ scrollTop: 0 }, 300);
    return false;
  });
  $(window).scroll(function () {
    if($(this).scrollTop() >= 200) {
      pageTop.fadeIn();
    } else {
      pageTop.fadeOut();
    }
  });
});

/*-------------underline-----------*/
$(window).scroll(function(){
  var windowHeight =$(window).height();
  var windowTop =$(window).scrollTop();
  $(".line_yb, .line_b, .line_b-s, .line_black-s").each(function(){
    var objectImage =$(this).offset().top;
    if(windowTop > objectImage - windowHeight*3/4){$(this).css("background-size","100% 100%")};
  });
});
</script>